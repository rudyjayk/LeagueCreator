<Window x:Class="league_final.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:league_final"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox HorizontalAlignment="Left" Margin="150,250,0,0" VerticalAlignment="Top" Width="130" Height="45" Loaded="comboBox_load" SelectionChanged="ComboBox_SelectionChanged"/>
        <Label Content="Select node to work on" HorizontalAlignment="Left" Height="45" Margin="381,250,0,0" VerticalAlignment="Top" Width="226" FontSize="18"/>
        <ComboBox HorizontalAlignment="Left" Margin="150,152,0,0" VerticalAlignment="Top" Width="130" Height="45" Loaded="ActionBox_load" SelectionChanged="ActionBox_SelectionChanged"/>
        <Label Content="Choose action" HorizontalAlignment="Left" Height="45" Margin="381,152,0,0" VerticalAlignment="Top" Width="226" FontSize="18"/>
        <Button Content="Next ->" HorizontalAlignment="Left" Height="42" Margin="445,346,0,0" VerticalAlignment="Top" Width="162" Click="Next_Click"/>

    </Grid>
</Window>
